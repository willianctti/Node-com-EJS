<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div class="content"> 
        <div>
            <% if (users.length > 0) { %>  <% users.forEach(user => {  %>

                <h3 class="titlle"><%= user.firstName %> <%= user.lastName %></h3>

                <p><%= user.email %></p>

                <% }) %>  <% } else { %>
                    <p>Não há usuários</p>
                    <% } %>
        </div>
    </div>

</body>
</html>